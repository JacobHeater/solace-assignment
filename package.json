{
  "name": "solace-candidate-assignment",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbo",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "db:generate": "drizzle-kit generate",
    "db:push": "drizzle-kit push",
    "db:migrate": "drizzle-kit migrate",
    "db:seed": "tsx --env-file=.env ./scripts/db/seed.ts",
    "db:initialize": "npm run db:generate && npm run db:migrate && npm run db:seed",
    "db:initialize:no-seed": "npm run db:generate && npm run db:migrate"
  },
  "dependencies": {
    "drizzle-orm": "^0.32.1",
    "next": "^14.2.19",
    "postgres": "^3.4.4",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-toastify": "^11.0.5",
    "use-debounce": "^10.0.4"
  },
  "devDependencies": {
    "@types/node": "^20.14.12",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@types/readline-sync": "^1.4.8",
    "drizzle-kit": "^0.23.0",
    "eslint": "^8.57.0",
    "eslint-config-next": "14.2.5",
    "postcss": "^8.4.40",
    "readline-sync": "^1.4.10",
    "tailwindcss": "^3.4.7",
    "tsx": "^4.19.4",
    "typescript": "^5.5.4"
  }
}
